// Basic object preposition rule (just test case equality and position
{
    "multi": {
        "clarifies": "#grammar",
        "tag-base": "#obj-preposition",
        "@self": {
            "clarifies": "#preposition",
            "clarify": {
                "tag": "#obj-preposition-link",
                "@equal-properties": {"1": ["case"]},
                "@position": {"1": "left"}
            }
        },
        "@other": {
            "clarifies": "#object"
        }
    }
}

// In
{
    "multi": {
        "clarifies": "#grammar",
        "tag-base": "#obj-preposition",
        "@self": {
            "clarifies": "#obj-preposition-link",
            "clarify": {
                "@word": "в",
                "@break": "obj-preposition-link[в]"
            }
        },
        "@other": {
        }
    }
}

// In
{
    "@continue": "obj-preposition-link[в]",
    "clarify": {
        "tag": "#hello",
        "@case": "prepositional",
        "@link": {
            "label": "#provides-prepositional-case"
        },
        "@other": {
            "@enable-props": "prepositional-case"
        }
    }
}

// In
{
    "@continue": "obj-preposition-link[в]",
    "clarify": {
        "@case": "accusative",
        "@link": {
            "label": "#provides-accusative-case"
        },
        "@other": {
