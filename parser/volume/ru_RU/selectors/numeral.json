{
    "selector": {
        "clarifies": "#term",
        "tag-base": "#count",
        "clarify": [
            {
                "tag": "#numeral",
                "@pos": ["numeral"]
            }
        ]
    }
}

#

{
    "multi": {
        "@self": {
            "clarifies": "#numeral",
            "clarify": {
                "tag": "#numeric-samecase",
                "@other": {
                    "or": {
                        "@case": [null],
                        "@equal-properties": {"0": ["case"]}
                    }
                }
            }
        },
        "@other": {
            "clarifies": "#numeral"
        }
    }
}

