    subclass:пруд
    subclass:болото

term:растительность
    mark:дикая растительность
    mark:городская растительность
    subclass:дерево
    subclass:трава
    subclass:водоросль

term:лес
    contains:дикая растительность
    contains:водоем

term:еда
    subclass:блюда
    subclass:растительность
